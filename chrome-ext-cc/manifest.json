{
    "manifest_version": 3,
    "name": "Credit Card Advisor",
    "version": "1.0",
    "action": {
      "default_popup": "src/popup.html"
    },
    "permissions": ["storage"],
    "host_permissions": ["https://*/*"],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
  }